{
  "caches": [
    {
      "cache": {
        "name": "redis-cache-1",
        "config": {
          "connection": {
            "type": "redis / guava / hazelcast / memcached",
            "host": "localhost",
            "port": "6379"
          },
          "data-store": {
            "connection": {
              "type": "mysql / phoenix / hbase",
              "host": "localhost",
              "port": "8888"
            },
            "load": "sync / async",
            "read": "through",
            "write": "sync / async"
          },
          "expiry-policy": {
            "ttl": {
              "count": 10,
              "unit": "seconds"
            },
            "entries": 1000,
            "size": {
              "count": 10,
              "unit": "megabytes"
            }
          }
        },
        "views": [
          {
            "view": {
              "name": "hashes-view-1",
